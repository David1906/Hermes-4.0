<suki:SukiWindow xmlns="https://github.com/avaloniaui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:suki="https://github.com/kikipoulet/SukiUI"
                 xmlns:main="clr-namespace:UI.Features.Main"
                 xmlns:operations="clr-namespace:UI.Features.Operations"
                 xmlns:panels="clr-namespace:UI.Features.Panels"
                 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                 x:Class="UI.Features.Main.MainWindow"
                 x:DataType="main:MainWindowViewModel"
                 Icon="/Assets/avalonia-logo.ico"
                 Title="UI">

    <suki:SukiWindow.Hosts>
        <suki:SukiToastHost Manager="{Binding ToastManager}" />
    </suki:SukiWindow.Hosts>

    <StackPanel Width="250"
                VerticalAlignment="Center"
                HorizontalAlignment="Center">

        <panels:PanelProcessorView DataContext="{Binding PanelProcessorViewModel }" />
    </StackPanel>
</suki:SukiWindow>